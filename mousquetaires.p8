function interact(x,y)
	if check_flag(1,x,y) then
		pick_up_key(x,y)
	elseif check_flag(2,x,y) 
	and p.keys>0 then
		open_door(x,y)
	end

	if x==22 and y==11 then
		create_msg("panneau2","ca marche")
	end
end	


